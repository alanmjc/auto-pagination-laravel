{
  "name": "alanmjc/auto-pagination-laravel",
  "description": "A Laravel package for handling pagination in API responses.",
  "type": "library",
  "require": {
    "php": "^7.3|^8.0",
    "illuminate/support": "^8.0|^9.0|^10.0"
  },
  "autoload": {
    "psr-4": {
      "AutoPaginationLaravel\\": "src/",
      "alanmjc/auto-pagination-laravel": "dev-main"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "scripts": {
    "test": "phpunit"
  }
}
